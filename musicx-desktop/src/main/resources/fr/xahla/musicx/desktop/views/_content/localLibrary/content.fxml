<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.come/fxml"
      fx:controller="fr.xahla.musicx.desktop.views._content.localLibrary.Content"
      styleClass="box-container"
      spacing="15" HBox.hgrow="ALWAYS" prefHeight="1200">
    <TabPane
            styleClass="tab-round"
            tabDragPolicy="REORDER"
            fx:id="localLibraryTab">
        <!-- Artists -->
        <Tab text="%localLibrary.tabArtists">

        </Tab>

        <!-- Albums -->
        <Tab text="%localLibrary.tabAlbums" fx:id="tabAlbums" />

        <!-- Album Genres -->
        <Tab text="%localLibrary.tabAlbumGenres">

        </Tab>

        <!-- Labels -->
        <Tab text="%localLibrary.tabLabels">

        </Tab>

        <!-- Playlists -->
        <Tab text="%localLibrary.tabPlaylists">

        </Tab>

        <!-- Songs -->
        <Tab text="%localLibrary.tabSongs" fx:id="tabSongs" />

        <!-- Song Genres -->
        <Tab text="%localLibrary.tabSongGenres">

        </Tab>

        <!-- Recently Added -->
        <Tab text="%localLibrary.tabRecentlyAdded">

        </Tab>
    </TabPane>
</VBox>